<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="co.sol.mapper.UMapper">

	<insert id="join">
		<selectKey keyProperty="u_no" order="BEFORE" resultType="int">
			select max(u_no)+1 from u_table
		</selectKey>
		insert into u_table values(u_no,)
		call pro_d_table(u_no,)
	</insert>
	
	
	<update id="Modify">
		update u_table set where u_no=#{u_no}
	</update>
	
	<!--  
	<delete id="Leave">
		delete from u_table where
	</delete>-->
	
	
	<select id="getId" resultType="String">
		select u_id from u_table where u_email = #{u_email}
	</select>
	
	
	<select id="getPw" resultType="String">
		select u_id from u_table where u_email = #{u_email} and u_id = #{u_id}
	</select>
	
	
	<select id="getUser" resultType="co.sol.main.UVO">
		select * from u_table where u_no = #{u_no}
	</select>
	
	
	<select id="Myboard" resultType="co.sol.main.BVO">
	
	</select>
	
	
	<select id="getScrap" resultType="co.sol.main.BVO">
	
	</select>
	
	
	<insert id="addDiscord">
		call pro_d_table(#{u_no}, #{d_weight}, #{d_height})
	</insert>
	
	

</mapper>

